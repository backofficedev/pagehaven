{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "server#dev": {
      "cache": false,
      "persistent": true
    },
    "server#dev:ready": {
      "cache": false,
      "persistent": false
    },
    "static#dev": {
      "dependsOn": ["server#dev:ready"],
      "cache": false,
      "persistent": true
    },
    "static#dev:ready": {
      "cache": false,
      "persistent": false
    },
    "web#dev": {
      "dependsOn": ["server#dev:ready"],
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "dependsOn": ["^deploy"],
      "cache": false
    },
    "server#destroy": {
      "dependsOn": ["web#destroy"],
      "cache": false
    },
    "static#destroy": {
      "dependsOn": ["web#destroy", "server#destroy"],
      "cache": false
    },
    "web#destroy": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "cache": false,
      "persistent": true
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "**/*.test.ts", "**/*.spec.ts"],
      "outputs": []
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "**/*.test.ts", "**/*.spec.ts"],
      "outputs": ["coverage/**"]
    }
  }
}
