{
  "name": "static",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    "./alchemy": {
      "bun": "./alchemy.run.ts",
      "types": "./alchemy.run.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc --noEmit",
    "dev": "env-cli -- alchemy dev --app STATIC_APP_NAME",
    "dev:ready": "wait-on tcp:3002",
    "deploy": "env-cli -- alchemy deploy --app STATIC_APP_NAME",
    "destroy": "env-cli -- alchemy destroy --app STATIC_APP_NAME",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@pagehaven/api": "workspace:*",
    "@pagehaven/auth": "workspace:*",
    "@pagehaven/infra": "workspace:*",
    "hono": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:",
    "@pagehaven/config": "workspace:*",
    "@types/node": "catalog:",
    "alchemy": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:testing",
    "wrangler": "catalog:"
  }
}
