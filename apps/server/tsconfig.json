{
  "extends": "@pagehaven/config/tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "env.d.ts"],
  "exclude": ["alchemy.run.ts", "dist", "node_modules"]
}
