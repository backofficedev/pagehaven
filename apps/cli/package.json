{
  "name": "@pagehaven/cli",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "pagehaven": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "dev:local": "bun run ./src/index.ts",
    "check-types": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@pagehaven/client": "workspace:*",
    "@pagehaven/utils": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "ora": "^9.0.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@pagehaven/config": "workspace:*",
    "@types/node": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
