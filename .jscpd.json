{
  "$schema": "https://json.schemastore.org/jscpd.json",
  "threshold": 2,
  "reporters": ["console"],
  "output": "./coverage/jscpd",
  "ignorePattern": [
    "vi\\.mock\\(\"[^\"]+\",\\s*(?:async\\s*)?\\(\\)\\s*=>\\s*(?:\\(\\{|\\{)[\\s\\S]*?(?:\\}\\)|\\})\\);",
    "vi\\.mocked\\([^)]+\\)\\.mockReturnValue\\(\\{[\\s\\S]*?\\}\\s*(?:as\\s+[^)]+)?\\);",
    "vi\\.mocked\\([^)]+\\)\\.mockImplementation\\([\\s\\S]*?\\);",
    "const\\s+\\{[^}]+\\}\\s*=\\s*await\\s+import\\([^)]+\\);",
    "const\\s+user\\s*=\\s*userEvent\\.setup\\(\\);",
    "await\\s+waitFor\\(\\(\\)\\s*=>\\s*\\{[\\s\\S]*?\\}\\);",
    "import\\s+\\{[^}]+\\}\\s+from\\s+['\"][^'\"]+['\"];",
    "import\\s+[A-Za-z]+\\s+from\\s+['\"][^'\"]+['\"];",
    "render\\(<[A-Za-z]+[^>]*/>\\);",
    "expect\\(screen\\.getBy[A-Za-z]+\\([^)]+\\)\\)\\.toBeInTheDocument\\(\\);",
    "expect\\(screen\\.getBy[A-Za-z]+\\([^)]+\\)\\)\\.toHaveValue\\([^)]+\\);",
    "expect\\([^)]+\\)\\.toHaveBeenCalledWith\\([\\s\\S]*?\\);",
    "expect\\([^)]+\\)\\.toEqual\\([\\s\\S]*?\\);",
    "\\.default\\(sql`\\(cast\\(unixepoch\\('subsecond'\\)\\s*\\*\\s*1000\\s+as\\s+integer\\)\\)`\\)",
    "\\.\\$onUpdate\\(\\(\\)\\s*=>\\s*(?:/\\*\\s*@__PURE__\\s*\\*/\\s*)?new\\s+Date\\(\\)\\)",
    "beforeEach\\(\\(\\)\\s*=>\\s*\\{[\\s\\S]*?\\}\\);",
    "afterEach\\(\\(\\)\\s*=>\\s*\\{[\\s\\S]*?\\}\\);",
    "setQueryResults\\(\\[[\\s\\S]*?\\]\\);",
    "mockUseQuery\\.mockReturnValue\\([\\s\\S]*?\\);",
    "describe\\(['\"][^'\"]+['\"],\\s*\\(\\)\\s*=>\\s*\\{",
    "it\\(['\"][^'\"]+['\"],\\s*(?:async\\s*)?\\(\\)\\s*=>\\s*\\{",
    "test\\(['\"][^'\"]+['\"],\\s*(?:async\\s*)?\\(\\)\\s*=>\\s*\\{",
    "test\\.step\\(['\"][^'\"]+['\"],\\s*async\\s*\\(\\)\\s*=>\\s*\\{",
    "await\\s+page\\.goto\\([^)]+\\);",
    "await\\s+page\\.click\\([^)]+\\);",
    "await\\s+page\\.fill\\([^)]+\\);",
    "await\\s+page\\.locator\\([^)]+\\)",
    "await\\s+expect\\(page[^)]+\\)\\.toBeVisible\\(\\);",
    "await\\s+expect\\(page[^)]+\\)\\.toContainText\\([^)]+\\);",
    "<Card[^>]*>[\\s\\S]*?</Card>",
    "<CardHeader>[\\s\\S]*?</CardHeader>",
    "<ConnectedFormField[^/]*/\\s*>",
    "<FormWrapper[^>]*>[\\s\\S]*?</FormWrapper>",
    "toast\\.(success|error)\\([^)]+\\);",
    "if\\s*\\(isPending\\)\\s*\\{\\s*return\\s*<Loader\\s*/>\\s*;\\s*\\}",
    "validators:\\s*\\{\\s*onSubmit:\\s*z\\.object\\(\\{[\\s\\S]*?\\}\\)\\s*,?\\s*\\}",
    "\\.handler\\(async\\s*\\(\\{\\s*input,\\s*context\\s*\\}\\)\\s*=>\\s*\\{",
    "const\\s+userId\\s*=\\s*context\\.session\\.user\\.id;",
    "const\\s+\\{\\s*deployment:\\s*dep\\s*\\}\\s*=\\s*await\\s+getDeploymentWithPermission\\(",
    "throw\\s+new\\s+Error\\(['\"][^'\"]+['\"]\\);",
    "return\\s+\\{\\s*success:\\s*true\\s*\\};",
    "await\\s+db\\s*\\.update\\([^)]+\\)\\s*\\.set\\(\\{[^}]+\\}\\)\\s*\\.where\\([^)]+\\);",
    "await\\s+db\\s*\\.select\\(\\{[^}]+\\}\\)\\s*\\.from\\([^)]+\\)\\s*\\.where\\([^)]+\\)\\s*\\.get\\(\\);",
    "await\\s+db\\s*\\.insert\\([^)]+\\)\\s*\\.values\\(\\{[\\s\\S]*?\\}\\);",
    "await\\s+db\\s*\\.delete\\([^)]+\\)\\s*\\.where\\([^)]+\\);",
    "\\.select\\(\\{[^}]+\\}\\)\\s*\\.from\\(",
    "and\\(eq\\([^)]+\\),\\s*eq\\([^)]+\\)\\)",
    "const\\s+prefix\\s*=\\s*`sites/\\$\\{[^}]+\\}/deployments/\\$\\{[^}]+\\}/`;",
    "const\\s+files\\s*=\\s*await\\s+listFiles\\(prefix\\);",
    "return\\s+files\\.map\\(\\(f\\)\\s*=>\\s*\\(\\{[\\s\\S]*?\\}\\)\\);",
    "<Tabs[^>]*>[\\s\\S]*?</Tabs>",
    "<TabsList>[\\s\\S]*?</TabsList>",
    "<TabsTrigger[^>]*>[^<]*</TabsTrigger>",
    "<TabsContent[^>]*>[^<]*</TabsContent>",
    "<DropdownMenu[^>]*>[\\s\\S]*?</DropdownMenu>",
    "<DropdownMenuTrigger[^>]*>[\\s\\S]*?</DropdownMenuTrigger>",
    "<DropdownMenuContent[^>]*>[\\s\\S]*?</DropdownMenuContent>",
    "<DropdownMenuItem[^>]*>[^<]*</DropdownMenuItem>",
    "screen\\.getByRole\\([^)]+\\)",
    "screen\\.getByText\\([^)]+\\)",
    "screen\\.queryByText\\([^)]+\\)",
    "await\\s+user\\.click\\([^)]+\\);",
    "await\\s+user\\.type\\([^)]+\\);",
    "fireEvent\\.[a-zA-Z]+\\([^)]+\\);"
  ],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/coverage/**",
    "**/*.min.js",
    "**/*.d.ts",
    "**/migrations/**",
    "**/routeTree.gen.ts",
    "**/alchemy.run.ts",
    "**/packages/api/src/lib/serve-static.ts"
  ],
  "format": ["typescript", "tsx", "javascript", "jsx"],
  "absolute": true,
  "gitignore": true
}
