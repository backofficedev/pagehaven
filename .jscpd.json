{
  "$schema": "https://json.schemastore.org/jscpd.json",
  "threshold": 12,
  "reporters": ["console"],
  "output": "./coverage/jscpd",
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/coverage/**",
    "**/*.min.js",
    "**/*.d.ts",
    "**/migrations/**",
    "**/routeTree.gen.ts",
    "**/alchemy.run.ts",
    "**/packages/api/src/lib/serve-static.ts",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/e2e/**"
  ],
  "format": ["typescript", "tsx", "javascript", "jsx"],
  "absolute": true,
  "gitignore": true
}
