{
  "$schema": "https://json.schemastore.org/jscpd.json",
  "threshold": 1,
  "reporters": ["console"],
  "output": "./coverage/jscpd",
  "ignorePattern": [
    "import\\s+\\{[^}]+\\}\\s+from\\s+['\"][^'\"]+['\"];",
    "import\\s+[A-Za-z]+\\s+from\\s+['\"][^'\"]+['\"];",
    "\\.default\\(sql`\\(cast\\(unixepoch\\('subsecond'\\)\\s*\\*\\s*1000\\s+as\\s+integer\\)\\)`\\)",
    "\\.\\$onUpdate\\(\\(\\)\\s*=>\\s*(?:/\\*\\s*@__PURE__\\s*\\*/\\s*)?new\\s+Date\\(\\)\\)",
    "describe\\(['\"][^'\"]+['\"],\\s*\\(\\)\\s*=>\\s*\\{",
    "it\\(['\"][^'\"]+['\"],\\s*(?:async\\s*)?\\(\\)\\s*=>\\s*\\{",
    "test\\(['\"][^'\"]+['\"],\\s*(?:async\\s*)?\\(\\)\\s*=>\\s*\\{",
    "test\\.step\\(['\"][^'\"]+['\"],\\s*async\\s*\\(\\)\\s*=>\\s*\\{",
    "vi\\.mock\\(['\"][^'\"]+['\"],\\s*\\(\\)\\s*=>\\s*\\{",
    "beforeEach\\(\\(\\)\\s*=>\\s*\\{\\s*vi\\.clearAllMocks\\(\\);\\s*\\}\\);",
    "Card:\\s*\\(\\{\\s*children",
    "CardContent:\\s*\\(\\{\\s*children",
    "CardHeader:\\s*\\(\\{\\s*children",
    "CardTitle:\\s*\\(\\{\\s*children",
    "CardDescription:\\s*\\(\\{\\s*children",
    "Button:\\s*\\(\\{\\s*children",
    "Skeleton:\\s*\\(\\{\\s*className",
    "\\)\\s*=>\\s*\\(\\s*<div\\s+className=\\{className\\}\\s+data-testid=",
    "\\)\\s*=>\\s*\\(\\s*<button\\s+className=\\{className\\}"
  ],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/coverage/**",
    "**/*.min.js",
    "**/*.d.ts",
    "**/migrations/**",
    "**/routeTree.gen.ts",
    "**/alchemy.run.ts",
    "**/*.test.tsx",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "format": ["typescript", "tsx", "javascript", "jsx"],
  "absolute": true,
  "gitignore": true
}
